
fun @main(): i32 {
@entry:
  %0 = alloc i32
  store 0, %0 // @a
  %1 = alloc i32
  store 1, %1 // @b

  %2 = load %0
  %3 = load %1

  %4 = alloc i32
  store 0, %4
  br %2, @then1, @else1

@then0:
  %5 = load %0
  %6 = load %1
  %7 = add %5, %6
  store %7, %0
  jump @end0

@else0:

@end0:
  %8 = load %0
  ret %8

@then1:
  %9 = ne %3, 0
  store %9, %4
  jump @end1

@else1:
  jump @end1

@end1:
  %10 = load %4
  br %10, @then0, @else0
}